{
  "name": "responsive-jsx-email",
  "version": "0.0.5",
  "description": "A utility for writing responsive email templates with jsx-email",
  "keywords": [
    "responsive-email",
    "react-email",
    "email-template",
    "email",
    "email-tables"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codeskills-dev/responsive-react-email.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./single-column": "./dist/single-column/index.js",
    "./dual-column": "./dist/dual-column/index.js",
    "./triple-column": "./dist/triple-column/index.js"
  },
  "typesVersions": {
    "*": {
      "single-column": [
        "dist/single-column/index.d.ts"
      ],
      "dual-column": [
        "dist/dual-column/index.d.ts"
      ],
      "triple-column": [
        "dist/triple-column/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "author": {
    "name": "Paul Ehikhuemen",
    "url": "https://github.com/lordelogos"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "18.2.14",
    "jsx-email": "^1.10.12",
    "react": "18.2.0",
    "tsup": "^7.0.0",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "react": "18.x"
  }
}
